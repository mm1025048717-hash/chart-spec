{
  "cleanUrls": true,
  "trailingSlash": false,
  "buildCommand": "",
  "outputDirectory": ".",
  "installCommand": "",
  "framework": null,
  "rewrites": [
    {
      "source": "/mobile",
      "destination": "/mobile.html"
    },
    {
      "source": "/mobile-spec",
      "destination": "/mobile-spec.html"
    },
    {
      "source": "/data-agent",
      "destination": "/data-agent.html"
    }
  ],
  "redirects": [
    {
      "source": "/mobile/mobile-spec",
      "destination": "/mobile-spec",
      "permanent": true
    },
    {
      "source": "/mobile/data-agent",
      "destination": "/data-agent",
      "permanent": true
    },
    {
      "source": "/mobile/:path*",
      "destination": "/:path*",
      "permanent": true
    }
  ]
}
